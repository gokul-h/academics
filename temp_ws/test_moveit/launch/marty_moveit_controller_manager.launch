<launch>
<!-- loads moveit_controller_manager on the parameter server
which is taken as argument
if no argument is passed, moveit_simple_controller_manager will
be set -->
<arg name="moveit_controller_manager" default="moveit_simple_
controller_manager/MoveItSimpleControllerManager" />
<param name="moveit_controller_manager" value="$(arg moveit_
controller_manager)"/>
<!-- loads ros_controllers to the param server -->
<rosparam file="$(find find test_moveit)/config/ros_
controllers.yaml"/>
</launch>